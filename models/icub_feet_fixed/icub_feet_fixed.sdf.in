<?xml version='1.0'?>
<sdf version='1.5'>
  <model name="iCub_feet_fixed">
    <include>
      <uri>model://@ROBOT_NAME@</uri>
      <pose>0.0 0 0.63 0 0 3.14</pose>
    </include>

    <joint name="fixed right to ground" type="revolute">
      <parent>world</parent>
      <child>iCub::base_link::r_foot</child>
       <axis>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
        <xyz>0 0 1</xyz>
        </axis>
    </joint>
    <joint name="fixed left to ground" type="revolute">
      <parent>world</parent>
      <child>iCub::base_link::l_foot</child>
       <axis>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
        <xyz>0 0 1</xyz>
        </axis>
    </joint>

  </model>
</sdf>
